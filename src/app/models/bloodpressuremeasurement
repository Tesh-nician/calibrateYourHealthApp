

export class  BloodPressureMeasurement {
  bloodPressureID: Number;
  systolicPressure: Number;
  diastolicPressure: Number;
  pulse: Number;
  date: String; // ISO date string
  bloodPressureComment?: String;
  patientid: Number;

  constructor(bloodPressureID: Number
    , systolicPressure: Number
    , diastolicPressure: Number
    , pulse: Number
    , date: String
    , bloodPressureComment: String
    , patientid: Number) 
    {
    this.bloodPressureID = bloodPressureID;
    this.systolicPressure = systolicPressure;
    this.diastolicPressure = diastolicPressure;
    this.pulse = pulse;
    this.date = date;
    this.bloodPressureComment = bloodPressureComment;
    this.patientid = patientid;
  }
}