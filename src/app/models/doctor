export class Doctor {
  doctorID: Number;
  firstName: String;
  lastName: String;
  rizivNumber: Number;
  password: String;
  username: String;
  confirmPassword: String;

  constructor(doctorID: Number
    , firstName: String
    , lastName: String
    , rizivNumber: Number
    , password: String
    , username: String
    , confirmPassword: String) {
    this.doctorID = doctorID;
    this.firstName = firstName;
    this.lastName = lastName;
    this.rizivNumber = rizivNumber;
    this.password = password;
    this.username = username;
    this.confirmPassword = confirmPassword;
  }
}