
//import patient from the models class



export class  WeightMeasurement {
  weightID: Number;
  weight: Number;
  weightComment?: String;
  date: String; // ISO date string
  patientid: Number;

  constructor(weightID: Number
    , weight: Number
    , weightComment: String
    , date: String
    , patientid: Number) 
    {
    this.weightID = weightID;
    this.weight = weight;
    this.weightComment = weightComment;
    this.date = date;
    this.patientid = patientid;
  }

}

 